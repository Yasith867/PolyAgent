{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        }
    ],
    "functions": {
        "api/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}